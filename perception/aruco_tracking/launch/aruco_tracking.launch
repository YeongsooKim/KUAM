<?xml version="1.0"?>
<launch>
    <include file="$(find usb_cam)/launch/usb_cam.launch"/>

        <arg name="arcuo_parser" default="$(find aruco_tracking)/../../config/" />
        <arg name="dictionary_id" default="0" />
        <arg name="input_video" default="missing" />
        <arg name="camera_id" default="0" />
        <arg name="instrinsic" default="$(find aruco_tracking)/../../config/calib.txt" />
        <arg name="marker_length" default="0.1" />
        <arg name="marker_detector_param" default="$(find aruco_tracking)/../../config/detector_params.yml" />
        <arg name="show_rejected_candidates" default="false" />
        <arg name="coner_refinement" default="0" />

    <node pkg="aruco_tracking" type="aruco_tracking_node" name="aruco_tracking" cwd="node" output="screen">
        <param name="arcuo_parser" value="$(arg arcuo_parser)" />
        <param name="dictionary_id" value="$(arg dictionary_id)" />
        <param name="input_video" value="$(arg input_video)" />
        <param name="camera_id" value="$(arg camera_id)" />
        <param name="instrinsic" value="$(arg instrinsic)" />
        <param name="marker_length" value="$(arg marker_length)" />
        <param name="marker_detector_param" value="$(arg marker_detector_param)" />
        <param name="show_rejected_candidates" value="$(arg show_rejected_candidates)" />
        <param name="coner_refinement" value="$(arg coner_refinement)" />
    </node>
</launch>
